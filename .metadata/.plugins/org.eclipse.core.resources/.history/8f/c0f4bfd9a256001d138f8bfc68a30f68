/**
 * 1. Reverse the given String
 * 2. Display Array
 */

package com.testng.scripts;

import org.testng.annotations.Test;

public class TC001 {
	@Test(priority = 2)
	public void funMethod1() {
		String nm = "TestngTest";
		StringBuffer sb = new StringBuffer(nm);
		StringBuffer sb1 = sb.reverse();

		System.out.println(sb1);
  }

	@Test(priority = 1)
	public void funMethod2() {
		int arr[] = { 10, 20, 30, 40 };
		for (int i : arr) {
			System.out.println(i);
		}
	}
	
	@Test(enabled = false)
	public void funMethod3() {
		
	}
}
